{
  "name": "suitsync-full",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:server": "npx prisma generate && node server/index.js",
    "postinstall": "prisma generate",
    "dev:frontend": "cd frontend && npm run dev -- -p 3001",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "seed": "node prisma/seed.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:frontend\"",
    "demo": "npm run prisma:migrate && npm run seed && npm run dev",
    "predev": "kill -9 $(lsof -ti:3000) 2>/dev/null || true && kill -9 $(lsof -ti:3001) 2>/dev/null || true"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/timegrid": "^6.1.17",
    "@prisma/client": "^6.9.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "concurrently": "^8.2.2",
    "connect-sqlite3": "^0.9.15",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.18.1"
  },
  "devDependencies": {
    "prisma": "^6.9.0"
  }
}
