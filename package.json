{
  "name": "suitsync-full",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:server": "nodemon server/index.js",
    "postinstall": "prisma generate",
    "dev:frontend": "cd frontend && npm run dev -- -p 3001",
    "dev": "echo \"\\n>>> Please run 'npm run dev:server' and 'npm run dev:frontend' in two separate terminals. <<<\\n\"",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm run start",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:studio": "npx prisma studio",
    "seed": "node prisma/seed.js",
    "lint": "cd frontend && npm run lint",
    "test": "cd frontend && npm run test",
    "demo": "npm run prisma:migrate && npm run seed && npm run dev",
    "predev": "pkill -f '(node|next|nodemon)' || true"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.17",
    "@prisma/client": "^6.10.1",
    "@prisma/extension-accelerate": "^2.0.1",
    "@sentry/node": "^9.30.0",
    "axios": "^1.9.0",
    "axios-retry": "^4.5.0",
    "bcryptjs": "^3.0.2",
    "connect-sqlite3": "^0.9.15",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "express-slow-down": "^2.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "react-big-calendar": "^1.19.4",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "kill-port": "^2.0.1",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prisma": "^6.10.1"
  }
}
